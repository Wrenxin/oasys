<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.gson.oasys.mappers.reportview.RepoetViewMapper">

	<select id="findAll" resultType="cn.gson.oasys.model.entity.workreport.Report">
		SELECT
		report_id,
		report_name,
		report_content,
		report_time
		FROM
		w_report
		ORDER BY
		report_id DESC
	</select>
</mapper>